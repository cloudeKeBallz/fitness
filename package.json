  },
  "license": "MIT",
  "engines": {
    "node": ">=4.3.2"
  },
  "scripts": {
    "format": "prettier --write --config .prettierrc.yaml '**/*.ts'",
    "clean": "rm -f src/*.js",
    "build": "yarn clean && tsc",
    "package": "yarn build && serverless package",
    "deploy": "yarn build && eval \"`aws-auth-helper` serverless deploy\""
